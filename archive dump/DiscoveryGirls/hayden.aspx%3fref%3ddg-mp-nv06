		function windowOpen(horse) {
			if (horse != "hayden") {
				horse = horse + ".html";
				window.open(horse,'EANITHING','toolbar=no,location=no, directories=no, status=yes, menubar=no, resizable=yes, copyhistory=no, scrollbars=no, width=600, height=400');
			} else {
				horse = "../flash/quest/HaydenPlayer.swf";
				window.open(horse,'EANITHING','toolbar=no,location=no, directories=no, status=yes, menubar=no, resizable=yes, copyhistory=no, scrollbars=no, width=320, height=280');
			}
		}
	
	</script>
	
