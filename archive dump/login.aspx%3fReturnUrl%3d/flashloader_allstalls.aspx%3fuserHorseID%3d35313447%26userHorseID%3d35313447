        function setLocalOffset() {
            var date = new Date();
            var offset = -date.getTimezoneOffset();

            document.getElementById("ctl00_Main_utcOffset").value = offset;
        }
        function getKeyCode(e) {
            if (e.keyCode == 13) {
                document.getElementById('ctl00_Main_Login1_hlnkLogin1').focus();
            }
            if (e.which == 13) {
                document.getElementById('ctl00_Main_Login1_hlnkLogin1').focus();
            }


        }
    </script>

